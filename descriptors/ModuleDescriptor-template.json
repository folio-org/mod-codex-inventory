{
  "id": "${artifactId}-${version}",
  "name": "Codex wrapper for Inventory",
  "requires": [
    {
      "id": "instance-storage",
      "version": "3.0"
    },
    {
      "id": "contributor-types",
      "version": "1.0"
    },
    {
      "id": "instance-types",
      "version": "1.0"
    },
    {
      "id": "instance-formats",
      "version": "1.0"
    },
    {
      "id": "identifier-types",
      "version": "1.0"
    }
  ],
  "provides": [
    {
      "id": "codex",
      "version": "2.0",
      "interfaceType": "multiple",
      "handlers": [
        {
          "methods": ["GET"],
          "pathPattern": "/codex-instances"
        },
        {
          "methods": ["GET"],
          "pathPattern": "/codex-instances/{id}"
        }
      ]
    }
  ],
  "permissionSets": []
}
